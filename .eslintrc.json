{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-multi-spaces": "off",
        "linebreak-style": "off",
        "object-curly-spacing": "off",
        "eol-last": "off",
        "indent": "off",
        "comma-dangle": ["error", "never"],
        "quotes": ["error", "double"],
        "max-len": ["warn", 200],
        "semi": ["error", "always", { "omitLastInOneLineBlock": true }]
    }
}
